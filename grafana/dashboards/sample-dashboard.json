{
  "dashboard": {
    "id": null,
    "title": "Sample Postgres Dashboard",
    "tags": ["postgres", "sample"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Users",
        "type": "stat",
        "targets": [
          {
            "rawQuery": true,
            "rawSql": "SELECT COUNT(*) as count FROM users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            }
          }
        },
        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Total Orders",
        "type": "stat",
        "targets": [
          {
            "rawQuery": true,
            "rawSql": "SELECT COUNT(*) as count FROM orders",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            }
          }
        },
        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Orders by Status",
        "type": "piechart",
        "targets": [
          {
            "rawQuery": true,
            "rawSql": "SELECT status, COUNT(*) as count FROM orders GROUP BY status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "User Analytics",
        "type": "table",
        "targets": [
          {
            "rawQuery": true,
            "rawSql": "SELECT * FROM user_analytics ORDER BY total_spent DESC",
            "refId": "A"
          }
        ],
        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}

